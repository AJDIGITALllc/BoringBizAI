rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /audits/{docId} {
      allow read: if true; // demo; tighten later
      allow write: if true; // demo; tighten later
    }
    match /competitors/{docId} {
      allow read: if true; // demo; tighten later
      allow write: if true; // demo; tighten later
    }
    match /leads/{docId} {
      allow read, write: if request.auth != null;
    }
  }
}